@if(User.Identity.IsAuthenticated)
{
    <form method="post" class="form-inline" asp-page="/Account/Logout">
        Welcome: @User.Identity.Name
        <button type="submit" class="btn btn-dark px-5">Logout</button>
    </form>
}
else
{
    <a class="btn btn-dark px-5" asp-page="/Account/Login">Login</a>
}
